.pages{
  padding: 0 30rpx;
  margin-bottom: 120rpx;
}
.header{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.title{
  font-size: 48rpx;
  color: #333;
  font-weight: 400;
  font-family: "Dosis", "Source Sans Pro", "Helvetica Neue", Arial, sans-serif;
  margin: 30rpx 0;
}
.logo{
  width: 16px;
  height: 20px;
  margin: 10px 0 0;
}
.des{
  font-size: 28rpx;
  color: #455a64;
  margin-bottom: 60rpx;
}
.list-item{
  margin-bottom: 20rpx;
}
.about{
  font-size:12px;
  text-indent:2em;
  line-height:24px;
  margin-top:20px;
}
.github{
  font-size: 12px;
  color: #666;
  margin-top: 20px;
}